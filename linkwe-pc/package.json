{
  "name": "linkwechat",
  "version": "0.1.0",
  "description": "LinkWeChat",
  "license": "GPL-3.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:test": "vue-cli-service serve --ENV=test",
    "develop": "vue-cli-service build --ENV=development",
    "build": "vue-cli-service build",
    "test": "vue-cli-service build --ENV=test",
    "preview": "node build/index.js --preview",
    "report": "npm run build --report",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-wechat",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/LinkWeChat/link-wechat.git"
  },
